        <script><![CDATA[function cancelOrderConfirmation() {
    var dialog = new GlideModal('glide_modal_confirm', true, 400);
    dialog.setTitle(new GwtMessage().getMessage('Confirmation'));
	dialog.setWidth(275);
    dialog.setPreference('body', new GwtMessage().format("Cancel this order?"));
    dialog.setPreference('focusTrap', true);
    dialog.setPreference('onPromptComplete', doComplete);
    dialog.render();

    function doComplete() {
        gsftSubmit(null, g_form.getFormElement(), 'cancel_order');
    }
    return false;
}

if (typeof window == 'undefined')
    cancelOrder();

function cancelOrder() {
    current.revision_operation = sn_ind_tmt_orm.Constants.REVISION_OPERATION_CANCEL;
	current.setValue("state",sn_ind_tmt_orm.Constants.ORDER_STATE_ASSESSING_CANCELLATION);
    current.update();
    action.setRedirectURL(current);
}]]></script>
