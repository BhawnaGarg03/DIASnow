    // Add your code here
    gs.info("*******LIAM userCalled******");
    var createUserInLIAM = new LIAMAPI().getUserLIAM(current.order_line_item.order.contact, current.order_line_item.order.contact.email) + '';
    gs.info("**********LIAM createUserInLIAM**********" + createUserInLIAM);
	if(createUserInLIAM=='User Created SuccessFully!!')
		{
			current.work_notes="User is successfully Created.";
		}
	else if(createUserInLIAM=='User Updated SuccessFully!!')
		{
			current.work_notes="User details are successfully updated.";
		}
	else
