        <client_script><![CDATA[function sendDetails() {
    alert("The provided information will be sending to SOAR, Thank you!");
    var getAPIGEEToken = new GlideAjax('sn_ind_tmt_orm.SOARAPI');
	getAPIGEEToken.addParam('sysparm_name', 'getAPIGEEToken'); //Method
	//getAPIGEEToken.addParam('userId',user); //Parameters
	getAPIGEEToken.getXMLAnswer(getResponse);
	
	function getResponse(response){
		var parseResponse = JSON.parse(response);
		alert(parseResponse.access_token);
	}
    GlideDialogWindow.get().destroy();
}

function continueCancel() {
    //alert("Cancelled");
    GlideDialogWindow.get().destroy();
}]]></client_script>
