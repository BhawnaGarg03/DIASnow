        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
 //gs.addInfoMessage('into business rule');
	var grCharOpt = current.characteristic_option.getDisplayValue();
 //  gs.addInfoMessage('charct value ' + grCharOpt);

    var grCustTask = new GlideRecord('sn_ind_tmt_orm_order_task');
    grCustTask.addQuery('order_line_item', current.order_line_item);

    grCustTask.query();
    if (grCustTask.next()) {
        var grCreatedDT = grCustTask.sys_created_on;
      //  gs.addInfoMessage('createdon date ' + grCreatedDT);

        var gdt = new GlideDateTime(grCreatedDT);
        gdt.addMonthsLocalTime(grCharOpt);
        gs.addInfoMessage('months added1' + gdt.addMonthsLocalTime(grCharOpt));

        var grCustorder = new GlideRecord('sn_ind_tmt_orm_order');
        grCustorder.addQuery('sys_id', current.order_line_item.order);

        grCustorder.query();
        if (grCustorder.next()) {
            gs.addInfoMessage('into 2nd if');
            grCustorder.expected_end_date = gdt.getLocalDate();
            gs.addInfoMessage('end date is' + grCustorder.expected_end_date);
        }


    }

    grCustorder.update();



})(current, previous);
]]></script>
